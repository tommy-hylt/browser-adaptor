{
  "manifest_version": 3,
  "name": "Browser Adaptor (CDP Bridge)",
  "version": "0.0.1",
  "description": "Bridges Chrome DevTools Protocol (chrome.debugger) to a local WebSocket server.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "debugger",
    "tabs",
    "bookmarks",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Browser Adaptor"
  }
}
